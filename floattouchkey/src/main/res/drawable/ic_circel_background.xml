<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--circel outer-->
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/colorDarkgray"/>
        </shape>
    </item>

    <!--circel inner-->
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/colorlargeBlack"/>
        </shape>
    </item>

    <!-- circel mid-->
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/colorHalfBlack"/>
        </shape>
    </item>

</layer-list>